{
  "version": 1,
  "projects": {
    "gql": {
      "projectType": "library",
      "root": "libs/gql",
      "sourceRoot": "libs/gql/src",
      "prefix": "odst",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/gql"
          ],
          "options": {
            "jestConfig": "libs/gql/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/gql/src/**/*.ts",
              "libs/gql/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "types": {
      "root": "libs/types",
      "sourceRoot": "libs/types/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/types/**/*.ts"
            ]
          }
        }
      },
      "tags": []
    },
    "waypoint": {
      "projectType": "application",
      "root": "apps/waypoint",
      "sourceRoot": "apps/waypoint/src",
      "prefix": "odst",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/waypoint",
            "index": "apps/waypoint/src/index.html",
            "main": "apps/waypoint/src/main.ts",
            "polyfills": "apps/waypoint/src/polyfills.ts",
            "tsConfig": "apps/waypoint/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/waypoint/src/favicon.ico",
              "apps/waypoint/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/waypoint/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/waypoint/src/environments/environment.ts",
                  "with": "apps/waypoint/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "waypoint:build:production"
            },
            "development": {
              "browserTarget": "waypoint:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/waypoint/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "waypoint:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/waypoint/src/**/*.ts",
              "apps/waypoint/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/waypoint"
          ],
          "options": {
            "jestConfig": "apps/waypoint/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "waypoint-api": {
      "root": "apps/waypoint-api",
      "sourceRoot": "apps/waypoint-api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/waypoint-api",
            "main": "apps/waypoint-api/src/main.ts",
            "tsConfig": "apps/waypoint-api/tsconfig.app.json",
            "tsPlugins": [
              "@nestjs/graphql/plugin"
            ],
            "assets": [
              "apps/waypoint-api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/waypoint-api/src/environments/environment.ts",
                  "with": "apps/waypoint-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "waypoint-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/waypoint-api/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/waypoint-api"
          ],
          "options": {
            "jestConfig": "apps/waypoint-api/jest.config.js",
            "passWithNoTests": true
          }
        },
        "prisma-generate": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "prisma generate --schema=apps/waypoint-api/schema.prisma"
          }
        },
        "prisma-migrate": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "env-cmd -f apps/waypoint-api/.env.local prisma db push --accept-data-loss",
            "schema": "apps/waypoint-api/schema.prisma"
          },
          "configurations": {
            "test": {
              "command": "env-cmd -f apps/waypoint-api/.env.test prisma db push --accept-data-loss"
            },
            "production": {
              "command": "env-cmd -f apps/waypoint-api/.env.prod prisma migrate dev"
            }
          }
        }
      },
      "tags": []
    },
    "waypoint-e2e": {
      "root": "apps/waypoint-e2e",
      "sourceRoot": "apps/waypoint-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/waypoint-e2e/cypress.json",
            "devServerTarget": "waypoint:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "waypoint:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/waypoint-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "waypoint"
      ]
    }
  }
}
