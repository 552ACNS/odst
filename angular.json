{
  "version": 2,
  "projects": {
    "auth": "libs/auth",
    "gql": {
      "projectType": "library",
      "root": "libs/gql",
      "sourceRoot": "libs/gql/src",
      "prefix": "odst",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/gql/src/**/*.ts",
              "libs/gql/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/gql"],
          "options": {
            "jestConfig": "libs/gql/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "helpers": {
      "root": "libs/helpers",
      "sourceRoot": "libs/helpers/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/helpers/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/helpers"],
          "options": {
            "jestConfig": "libs/helpers/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "ods": {
      "projectType": "application",
      "root": "apps/ods",
      "sourceRoot": "apps/ods/src",
      "prefix": "odst",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ods",
            "index": "apps/ods/src/index.html",
            "main": "apps/ods/src/main.ts",
            "polyfills": "apps/ods/src/polyfills.ts",
            "tsConfig": "apps/ods/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/ods/src/favicon.ico", "apps/ods/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/ods/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/ods/src/environments/environment.ts",
                  "with": "apps/ods/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ods/src/**/*.ts",
              "apps/ods/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ods"],
          "options": {
            "jestConfig": "apps/ods/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ods:build:production"
            },
            "development": {
              "browserTarget": "ods:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4210,
            "proxyConfig": "apps/ods/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ods:build"
          }
        }
      },
      "tags": []
    },
    "ods-api": {
      "root": "apps/ods-api",
      "sourceRoot": "apps/ods-api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ods-api",
            "main": "apps/ods-api/src/main.ts",
            "tsConfig": "apps/ods-api/tsconfig.app.json",
            "tsPlugins": ["@nestjs/graphql/plugin"],
            "assets": ["apps/ods-api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/ods-api/src/environments/environment.ts",
                  "with": "apps/ods-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/ods-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ods-api"],
          "options": {
            "jestConfig": "apps/ods-api/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "ods-api:build"
          }
        },
        "prisma-generate": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx prisma generate --schema=apps/ods-api/schema.prisma"
          }
        },
        "prisma-migrate": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "env-cmd -f apps/ods-api/.env.local npx prisma db push --accept-data-loss --force-reset",
              "env-cmd -f apps/ods-api/.env.local yarn ts-node ./apps/ods-api/prisma-seed.ts"
            ],
            "schema": "apps/ods-api/schema.prisma",
            "parallel": false
          },
          "configurations": {
            "test": {
              "command": "env-cmd -f apps/ods-api/.env.test npx prisma db push --accept-data-loss"
            },
            "production": {
              "command": "env-cmd -f apps/ods-api/.env.prod prisma migrate dev"
            }
          }
        },
        "prisma-studio": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "env-cmd -f apps/ods-api/.env.local npx prisma studio",
            "schema": "apps/ods-api/schema.prisma"
          }
        }
      },
      "graphql-generate": {
        "builder": "@nrwl/workspace:run-commands",
        "options": {
          "commands": [
            "ts-node -r tsconfig-paths/register --project apps/ods-api/tsconfig.app.json apps/ods-api/graphql-generate.ts"
          ]
        }
      },
      "tags": []
    },
    "ods-e2e": {
      "root": "apps/ods-e2e",
      "sourceRoot": "apps/ods-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ods-e2e/cypress.json",
            "devServerTarget": "ods:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ods:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/ods-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["ods"]
    },
    "shared-angular": {
      "projectType": "library",
      "root": "libs/shared/angular",
      "sourceRoot": "libs/shared/angular/src",
      "prefix": "odst",
