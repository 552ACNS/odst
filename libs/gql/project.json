      }
    },
    "build": {
      "options": {
        "tsConfig": "libs/gql/tsconfig.lib.json"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/gql"],
      "options": {
